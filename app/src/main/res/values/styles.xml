<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <declare-styleable name="ReportAboutView">
        <attr name="rav_titleIcon" format="reference" />
        <attr name="rav_contentText" format="string" />
        <attr name="rav_learnMoreUrl" format="string" />
    </declare-styleable>

    <declare-styleable name="DeviceContactTipView">
        <attr name="dctv_title" format="string"/>
        <attr name="dctv_image" format="reference"/>
        <attr name="dctv_tip" format="string"/>
    </declare-styleable>
</resources>
